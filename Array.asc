
int Array_SafeLength(ArrayElement *arr[]) {
	return arr != null ? arr.Length : 0;
}

bool Array_TryAdd(ArrayElement *arr[], ArrayElement *elem) {
	if (arr == null) {
		return -1;
	}
	int len = arr.Length;
	for (int i = 0; i < len; ++i) {
		if (arr[i] == null) {
			arr[i] = elem;
			return i;
		}
	}
	return -1;
}

void Array_Copy(ArrayElement *dest[], ArrayElement *src[], int count) {
	for (int i = 0; i < count; ++i) {
		dest[i] = src[i];
	}
}
